# Example environment configuration for DawlessStudio project
NODE_ENV=development
VITE_PORT=5173
# Base URL for backend API (replace with actual)
VITE_API_URL=http://localhost:3000/api

# PostgreSQL database connection URL
DATABASE_URL=postgres://user:password@localhost:5432/dawlessstudio

# JWT secret used for signing user sessions
JWT_SECRET=your_jwt_secret_here

# OpenAI API key for DawlessGPT integration
OPENAI_API_KEY=your_openai_api_key_here

# Discord OAuth credentials
DISCORD_CLIENT_ID=your_discord_client_id_here
DISCORD_CLIENT_SECRET=your_discord_client_secret_here

# GitHub repository details
GITHUB_REPOSITORY=Meshuggih/Meshuggih.github.io
